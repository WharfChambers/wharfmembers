<h1>
  Mailing List
  <%= link_to mailing_list_members_path(format: :csv) do %>
    <i class="icon-download-alt"></i>
  <% end %>
</h1>

<%= paginate @members %>
<%= form_tag bulk_action_members_path do %>
  <%= render 'table' %>
  <div class="actions">
    <%= submit_tag 'Delete Selected', name: 'delete-selected', class: 'btn btn-danger' %>
  </div>
<% end %>
<div class="pull-right">
  <%= paginate @members %>
</div>
